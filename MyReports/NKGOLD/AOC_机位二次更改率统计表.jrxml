	<property name="ireport.y" value="0"/>
	<queryString>
		<![CDATA[SELECT * FROM (
                  SELECT COUNT(*) AS allCnt,
                                TO_CHAR(pd_srtd- 5/24,'yyyy-mm-dd') AS flightDate
                  FROM pl_departure pd
                  WHERE (TO_CHAR(pd.pd_srtd- 5/24 ,'yyyy-mm-dd') BETWEEN  to_char(to_date(?,'yyyy-mm-dd hh24:mi:ss'),'yyyy-mm-dd') AND to_char(to_date(?,'yyyy-mm-dd hh24:mi:ss'),'yyyy-mm-dd'))
                        AND pd.pd_rfst_flightstatus !='X'
                        AND pd.pd_ral_airline !='8Y'
						AND pd.pd_ral_airline !='CF'
                        AND pd.pd_rstc_servicetypecode   NOT IN ('H/Z','H/Y','H/G','L/W','U/H')
                        AND pd.pd_rsta_stand IS NOT NULL
                   GROUP BY TO_CHAR(pd_srtd- 5/24,'yyyy-mm-dd')

       )b
       LEFT JOIN(
                 SELECT
                        flightdate1,
                        count(*) as cnt
                FROM(
                        SELECT count(*) as cntt,
                               pd_flightNumber        AS flightNumber,
                               TO_CHAR(pd_srtd- 5/24 ,'yyyy-mm-dd') as flightDate1
                        FROM sy_audittrail,pl_departure
                        WHERE (TO_CHAR(pd_srtd- 5/24 ,'yyyy-mm-dd') BETWEEN  to_char(to_date(?,'yyyy-mm-dd hh24:mi:ss'),'yyyy-mm-dd') AND to_char(to_date(?,'yyyy-mm-dd hh24:mi:ss'),'yyyy-mm-dd'))
                              AND pd_rfst_flightstatus !='X'
                              AND pd_pd_mainflight is null
                              AND pd_ral_airline !='8Y'
							  AND pd_ral_airline !='CF'
                              AND pd_rstc_servicetypecode   NOT IN ('H/Z','H/Y','H/G','L/W','U/H')
                              AND sat_masterkey   =pd_idseq
                              AND sat_mastertable   ='PL_DEPARTURE'
                              AND sat_changecode IN ('PD_RSTA_STAND.UPDATE')
                        GROUP BY  TO_CHAR(pd_srtd- 5/24 ,'yyyy-mm-dd'),pd_flightNumber
                        )a  WHERE a.cntt>2 GROUP BY flightdate1
         )c ON b.flightDate = c.flightDate1 ORDER BY flightDate]]>
	</queryString>
	<field name="ALLCNT" class="java.lang.Double"/>
	<field name="FLIGHTDATE" class="java.lang.String"/>
	<field name="FLIGHTDATE1" class="java.lang.String"/>
	<field name="CNT" class="java.lang.Double"/>
	<variable name="rate" class="java.lang.Double" resetType="Column">
		<variableExpression><![CDATA[$F{CNT}.doubleValue()/$F{ALLCNT}.doubleValue()]]></variableExpression>
		<initialValueExpression><![CDATA[new java.lang.Double(0)]]></initialValueExpression>
	</variable>
	<variable name="all" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$F{ALLCNT}]]></variableExpression>
		<initialValueExpression><![CDATA[new java.lang.Double(0)]]></initialValueExpression>
	</variable>
	<variable name="cntc" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$F{CNT}]]></variableExpression>
		<initialValueExpression><![CDATA[new java.lang.Double(0)]]></initialValueExpression>
	</variable>
	<variable name="allcntrate" class="java.lang.Double">
		<variableExpression><![CDATA[$V{cntc}.doubleValue()/$V{all}.doubleValue()]]></variableExpression>
		<initialValueExpression><![CDATA[new java.lang.Double(0)]]></initialValueExpression>
	</variable>
